                            type="nsBasisTypen:CEigenschaften_Datei"
                            minOccurs="1"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Metadaten der Binärdatei, die von der Balise gesendet wird</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="Eigenschaften_Binaerdatei_Hilfe"
                            type="nsBasisTypen:CEigenschaften_Datei"
                            minOccurs="0"
                            maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Metadaten der Hilfsdatei zur Unterstützung bei der Balisenprüfung.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ID_Anhang_EA_Doku"
                            type="xs:anyURI"
                            ecore:reference="nsBasisobjekte:CAnhang"
                            nillable="true"
                            minOccurs="1"
                            maxOccurs="1">
